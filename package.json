{
  "name": "Lynx",
  "productName": "Lynx",
  "version": "0.2.11",
  "description": "Electron application providing the graphical user interface for ECC",
  "scripts": {
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings ./test/runTests.js",
    "test-all": "npm run flow && npm run test && npm run build && npm run test-e2e",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings ./test/runTests.js e2e",
    "hot-updates-server": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js",
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --progress --profile --colors",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "prestart": "npm run build",
    "flow": "flow check",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "start-hot-renderer": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.dev",
    "postinstall": "npm rebuild --runtime=electron --target=1.8.8 --disturl=https://atom.io/download/atom-shell --build-from-source",
    "setup": "concurrently \"npm run flow-typed\" \"npm run build\" \"npm run build-dll\" \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "dev": "cross-env START_HOT=1 npm run hot-updates-server",
    "package-aa-win-32": "npm run build && build --win -c ./os/win32.json -p always",
    "package-aa-win-64": "npm run build && build --win --x64 -c ./os/win64.json -p always",
    "package-aa-all": "npm run package-aa-win-32 && npm run package-aa-win-64",
    "package-osx": "npm run build && build --mac --x64 -c ./os/osx.json",
    "package-win": "npm run build && build --win --x64 -c ./os/win64.json",
    "package-win-32": "npm run build && build --win --ia32 -c ./os/win32.json",
    "package-linux": "npm run build && build --linux -c ./os/lin64.json",
    "package-linux-32": "npm run build && build --linux --ia32 -c ./os/lin32.json",
    "package-all": "npm run package-win && npm run package-win-32 && npm run package-linux && npm run package-linux-32 && npm run package-osx"
  },
  "browserslist": "electron 1.6",
  "repository": "git+https://github.com/project-ecc/lynx.git",
  "author": "Eccoin Developers (https://github.com/project-ecc/lynx)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/project-ecc/lynx/issues"
  },
  "keywords": [
    "electron"
  ],
  "homepage": "https://github.com/project-ecc/lynx",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "setupFiles": [
      "./internals/scripts/CheckBuiltsExist.js"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^27.3.1",
    "babel-loader": "^7.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.3",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-flow-runtime": "^0.11.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "0.1.1",
    "chalk": "^2.0.1",
    "concurrently": "^3.5.0",
    "cross-env": "^5.2.1",
    "cross-spawn": "^5.1.0",
    "css-loader": "^2.1.1",
    "electron": "^1.8.8",
    "electron-builder": "^19.8.0",
    "electron-devtools-installer": "^2.2.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "fbjs-scripts": "^1.2.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.48.0",
    "flow-runtime": "^0.13.0",
    "flow-typed": "^3.4.0",
    "fs-path": "latest",
    "html-webpack-plugin": "^2.29.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jsdom": "^11.0.0",
    "minimist": "^1.2.5",
    "node-sass": "^6.0.1",
    "prop-types": "^15.6.0",
    "react-addons-test-utils": "^15.0.6",
    "react-test-renderer": "^17.0.2",
    "redux-logger": "^3.0.6",
    "request": "^2.88.2",
    "rimraf": "^2.7.1",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.5",
    "spectron": "^15.0.0",
    "url-loader": "^1.1.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "bootstrap": "^4.6.1",
    "bootstrap-material-design": "^0.5.10",
    "checksum": "^0.1.1",
    "child_process": "^1.0.2",
    "devtron": "^1.4.0",
    "eccoin-js": "git://github.com/project-ecc/eccoin-js.git",
    "electron-debug": "^1.2.0",
    "electron-download-manager": "git+https://github.com/project-ecc/electron-download-manager.git",
    "electron-settings": "^3.1.1",
    "electron-updater": "^2.20.2",
    "events": "^1.1.1",
    "extract-zip": "^1.7.0",
    "font-awesome": "^4.7.0",
    "fs-path": "latest",
    "glob": "^7.2.0",
    "history": "^4.10.1",
    "jquery": "^3.5.1",
    "lowdb": "^0.16.2",
    "node-powershell": "^3.3.1",
    "path": "^0.12.7",
    "popper.js": "^1.16.1",
    "project-version": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-loader-display": "^3.1.0",
    "react-loading": "^0.1.4",
    "react-motion": "^0.5.2",
    "react-progressbar.js": "^0.2.0",
    "react-redux": "^5.1.2",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-smooth-collapse": "^1.3.2",
    "redux": "^3.7.1",
    "redux-thunk": "^2.4.0",
    "request-progress": "^3.0.0",
    "semver": "^7.3.4",
    "source-map-support": "^0.4.15",
    "style-loader": "^1.2.1",
    "tar": "^4.4.19"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x",
    "yarn": ">=0.21.3"
  },
  "dev-main": "index.js",
  "main": "./app/main.prod.js"
}
